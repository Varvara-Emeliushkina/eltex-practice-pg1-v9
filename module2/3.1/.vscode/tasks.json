{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-windows",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g3",
                "0g",
                "-o",
                "file_permissions.exe",
                "main.c",
                "ui.c",
                "file_operations.c",
                "permissions.c",
                "-Wall",
                "-Wextra"
            ],
            "group": "build",
            "problemMatcher": [
                "$gcc"
            ],
            "options": {
                "shell": {
                    "executable": "cmd.exe",
                    "args": [
                        "/C"
                    ]
                }
            }
        },
        {
            "type": "cppbuild",
            "label": "C/C++: gcc.exe build active file",
            "command": "C:\\msys64\\ucrt64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-g",
                "${D:\\3.1\\file_permissions.exe}",
                "-o",
                "${D:\\3.1\\}\\${file_permissions}.exe"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": "build",
            "detail": "Task generated by Debugger."
        }
    ]
}